{
    "name": "quickbooks/v3-php-sdk",
    "description": "The Official PHP SDK for QuickBooks Online Accounting API",
    "type": "library",
    "keywords": ["api", "http", "rest", "quickbooks", "smallbusiness"],
    "homepage": "http://developer.intuit.com",
    "require": {
		"php": ">=5.6.0",
		"rkaiser0324/XSD-to-PHP": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
	},
    "suggest": {
        "ext-curl": "Uses Curl to make HTTP Requests",
        "guzzlehttp/guzzle": "Uses Guzzle to make HTTP Requests"
    },
    "autoload": {
        "psr-4": {
			"QuickBooksOnline\\API\\": "src/",
			"" : "vendor/rkaiser0324/XSD-to-PHP/src/"
        }
	},
	"repositories": [
		{
		"url": "https://github.com/rkaiser0324/XSD-to-PHP.git",
		"type": "git"
		}
	],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "hlu2",
            "email": "Hao_Lu@intuit.com"
        }
    ],
    "scripts": {
        "phpstan": "vendor/bin/phpstan --memory-limit=1G --level=0 analyse src",
        "test": "phpunit --configuration phpunit.xml"
    }
}
